{
  "name": "photo-site",
  "private": true,
  "version": "1.0.0",
  "description": "A fullstack example of a pnpm workspace.",
  "scripts": {
    "dev": "pnpm dev:compose && pnpm --stream -r run dev",
    "dev:compose": "docker-compose up",
    "lint": "tsc --build && pnpm eslint . --fix",
    "eslint": "eslint --ext .mjc,.cjs,.js,.jsx,.ts,.tsx,.graphql",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{mjs,cjs,js,jsx,ts,tsx,json,graphql}": [
      "eslint --fix",
      "tsc --noEmit"
    ]
  },
  "keywords": [],
  "author": "spahi43@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@types/stylelint": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0-alpha.0",
    "@typescript-eslint/parser": "^7.0.0-alpha.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-sql": "^2.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "prettier-plugin-sql": "^0.15.1",
    "typescript": "^5.2.0-beta"
  },
  "pnpm": {
    "patchedDependencies": {
      "@remix-run/router@1.7.2": "patches/@remix-run__router@1.7.2.patch"
    },
    "overrides": {
      "@apollo/client": "^3.8.0-rc.2",
      "@swc/core": "^1.3.74"
    },
    "allowNonAppliedPatches": true
  }
}
